<p id="notice"><%= notice %></p>

<h1>Listing Sponsors</h1>
<div class="table_div">
<table>
  <thead>
    <tr>
      <th>Homepage</th>
      <th>Competition</th>
      <th>Team season</th>
      <th>Name</th>
      <th>Description</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors.each do |sponsor| %>
      <tr>
        <td><%= sponsor.homepage %></td>
        <td><%= sponsor.competition_id %></td>
        <td><%= sponsor.team_season_id %></td>
        <td><%= sponsor.name %></td>
        <td><%= sponsor.description %></td>
        <td><%= sponsor.url %></td>
        <td><%= link_to 'Show', sponsor %></td>
        <td><%= link_to 'Edit', edit_sponsor_path(sponsor) %></td>
        <td><%= link_to 'Destroy', sponsor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Sponsor', new_sponsor_path %>
