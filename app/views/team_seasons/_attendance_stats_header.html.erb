<% get_states_options().each do |state| %>
  <% if state[1] > state_presence() && state[1] < state_called_no_answer() %>
    <th class="th-total"><%= get_state_initial(state[1].to_i)%></th>
  <% elsif state[1] == state_presence() %>
    <th class="th-total-first"><%= get_state_initial(state[1].to_i)%></th>
  <% else %>
    <th class="th-total"></th>
  <% end %>
<% end %>

